    {
        "id": "bc8b0759248d8adc",
        "type": "function",
        "z": "47f96696952f8e7c",
        "name": "Procesar Datos",
        "func": "// Corrección para el nodo \"Procesar Datos\" en Node-RED\n\n// Extraer datos\nconst temperatura = msg.payload.temperatura || 0;\nconst ritmoCardiaco = msg.payload.ritmo_cardiaco || 0;\nconst spo2 = msg.payload.spo2 || 0;\nconst alertas = msg.payload.alertas || \"\";\nconst timestamp = msg.payload.timestamp || Date.now()/1000;\n\n// Formatear timestamp\nconst date = new Date(timestamp * 1000);\nconst timeStr = date.toLocaleTimeString();\n\n// Enviar datos a los gauges y gráficos\nconst msgTemp = { payload: temperatura, topic: \"temperatura\" };\nconst msgHR = { payload: ritmoCardiaco, topic: \"ritmo_cardiaco\" };\nconst msgSpo2 = { payload: spo2, topic: \"spo2\" };\n\n// Mensaje para alertas\nconst msgAlert = { \n    payload: alertas,\n    alertas: alertas && alertas.length > 0\n};\n\n// Datos para gráficos históricos\nconst msgChart = {\n    payload: [\n        { x: timeStr, y: temperatura, series: \"Temperatura\" },\n        { x: timeStr, y: ritmoCardiaco, series: \"Ritmo Cardíaco\" },\n        { x: timeStr, y: spo2, series: \"SpO2\" }\n    ]\n};\n\n// Mensaje para PostgreSQL\nconst msgPostgres = {\n    topic: \"INSERT INTO sensor_readings (temperatura, ritmo_cardiaco, spo2, alertas) VALUES ($1, $2, $3, $4);\",\n    params: [temperatura, ritmoCardiaco, spo2, alertas]\n};\n\n// Enviar múltiples mensajes\nreturn [msgTemp, msgHR, msgSpo2, msgAlert, msgChart, msgPostgres];",
